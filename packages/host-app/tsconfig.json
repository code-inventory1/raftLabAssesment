{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "es6"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": false,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "accountOverview/*": ["./src/types/modules.d.ts"],
      "transactionDetails/*": ["./src/types/modules.d.ts"]
    }
  },
  "include": [
    "src/**/*",
    "src/types/**/*"
  ],
  "exclude": ["node_modules", "dist"]
}